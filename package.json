{
  "name": "UvcCameraManager",
  "version": "1.0.0-0",
  "private": true,
  "scripts": {
    "rn": "adb reverse tcp:8081 tcp:8081; node node_modules/react-native/local-cli/cli.js start",
    "rn-fresh": "adb reverse tcp:8081 tcp:8081; watchman watch-del-all; node node_modules/react-native/local-cli/cli.js start --reset-cache",
    "android": "node node_modules/react-native/local-cli/cli.js run-android",
    "ios": "node node_modules/react-native/local-cli/cli.js run-ios",
    "web": "node node_modules/react-web/local-cli/cli.js start web/webpack.config.js",
    "log": "adb logcat *:S ReactNative:V ReactNativeJS:V",
    "debug-web-w": "NODE_ENV=development WEBPACK_WATCH=true webpack --config web/webpack.config.js -w",
    "build-web-w": "NODE_ENV=production WEBPACK_WATCH=true webpack --config web/webpack.config.js -w",
    "build-web": "./package.pre && NODE_ENV=production webpack --config web/webpack.config.js -p ; ./package.post",
    "build-ios": "./package.pre && react-native unbundle --entry-file index.ios.js --bundle-output main.jsbundle --platform ios --dev false ; ./package.post",
    "build-android": "./package.pre && cd android && ./gradlew assembleRelease ; cd .. ; ./package.post",
    "install-android": "./package.pre && cd android && ./gradlew installRelease ; cd .. ; ./package.post",
    "checkversion": "node node_modules/fbjs-scripts/node/check-dev-engines.js package.json",
    "postinstall": "npm run checkversion; ./post-install.sh",
    "test": "jest",
    "e2e-update-server-web": "webdriver-manager update --versions.standalone=3.7.1 --versions.gecko=v0.18.0 --versions.chrome=2.32",
    "e2e-server-web": "touch node_modules/webdriver-manager/selenium/standalone-response.xml; touch node_modules/webdriver-manager/selenium/chrome-response.xml; webdriver-manager start --versions.standalone=3.7.1 --versions.gecko=v0.18.0 --versions.chrome=2.32",
    "e2e-web": "codeceptjs run",
    "e2e-server-native": "appium",
    "e2e-android": "codeceptjs run --profile=android",
    "e2e-ios": "codeceptjs run --profile=ios",
    "watch": "webpack-dev-server --content-base public/"
  },
